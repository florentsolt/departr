{
    "manifest_version": 2,

    "name": "Departr",
    "description": "Departr is new tab replacement that display a nice picture.",
    "version": "1.7.0",

    "permissions": [ 
        "tabs",
        "geolocation",
        "https://query.yahooapis.com/*",
        "https://departr.solt.biz/*" 
    ],

    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "icons": {
        "128": "logo.png"
    }
}
